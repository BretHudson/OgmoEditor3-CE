{
    "name": "ogmoeditor",
    "version": "0.3.0",
    "description": "A general-case level editor for indie game developers!",
    "repository": "https://github.com/TheSpydog/OgmoEditor3-CE",
    "main": "app.js",
    "scripts": {
        "start": "electron bin/.",
        "build": "npm run scss && npm run assets",
        "scss": "node-sass style/core.scss -o bin",
        "assets": "cp -R assets/ bin/ && cp package.json bin/"
    },
    "devDependencies": {
        "electron": "^4.1.4",
        "electron-rebuild": "^1.8.4",
        "node-sass": "^4.12.0"
    },
    "dependencies": {
        "chokidar": "^2.1.5",
        "jquery": "^3.4.0",
        "klaw": "^3.0.0",
        "nsfw": "^1.2.2"
    }
}
